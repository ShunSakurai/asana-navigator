{
  "author": "Shun Sakurai",
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": ["main.css"],
      "js": ["content_script.js", "ui_translation.js"],
      "matches": ["https://app.asana.com/*"],
      "run_at": "document_idle"
    }
  ],
  "description" : "Display projects on top, go to previous/next subtasks, convert to a subtask of another task, replace text in the description, etc.",
  "homepage_url": "https://github.com/ShunSakurai/asana-navigator",
  "icons": {
    "128": "images/an128.png",
    "48": "images/an48.png",
    "16": "images/an16.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "name": "Asana Navigator",
  "offline_enabled": false,
  "options_page": "options.html",
  "permissions": [
    "https://app.asana.com/*",
    "activeTab", "storage"
  ],
  "short_name": "AsaNavigator",
  "version": "1.6.5"
}
